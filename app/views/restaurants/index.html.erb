 <h1>Restaurants:</h1>
 <tr>
    <% @restaurants.each do |restaurant| %>
    <tr class="text-center">
      <td>
      <ul>
        <li><%= link_to "#{restaurant.name}", restaurant_path(restaurant) %></li>
        </ul>
      </td>
    </tr>
    <% end %>
    <td>
        <%= link_to 'Add a restaurant', new_restaurant_path(@restaurant),
            class: 'btn btn-primary' %>
     </td>
</tr>
